dotenv: .env
wallets:
  from_key: ${PRIVATE_KEY}
dependencies:
  - smartcontractkit/chainlink-brownie-contracts@1.2.0
  - paulrberg/prb-math@2.5.0
  - OpenZeppelin/openzeppelin-contracts@4.7.3
  - chiru-labs/ERC721A@4.2.3
compiler:
  solc:
    version: 0.8.4
    remappings:
      - '@chainlink=smartcontractkit/chainlink-brownie-contracts@1.2.0'
      - '@prb=paulrberg/prb-math@2.5.0'
      - '@openzeppelin=OpenZeppelin/openzeppelin-contracts@4.7.3'
      - '@chirulabs=chiru-labs/ERC721A@4.2.3'
season:
  chicago:
    ticker: PA-C 
    max_supply: 40
    price:
      normal: 0.1
      wl: 0.07
  new_york:
    ticker: PA-NY
    max_supply: 1700
    price:
      normal: 0.09
      wl: 0.06
  detroit:
    ticker: PA-D
    max_supply: 2000
    price:
      normal: 0.08
      wl: 0.05
networks:
  goerli:
    # The location of the BTC_USD ChainLink contract on Goerli
    btc_usd_price_feed: '0xA39434A63A52E749F02807ae27335515BA4b07F7'
    marketing_address: '0x000000000000000000000000000000000example'
    nft_json_folder: '000000000000000000000000000000000000000example'
    nft_prereveal_json_folder: '0000000000000000000000000000000000000prereveal'
    accessories_address: '0x000000000000000000000000000000000000acce'
    accommodation_address: '0x000000000000000000000000000000000000acco'
  development:
    cmd_settings:
      accounts: 2001
    nft_json_folder: '000000000000000000000000000000000000000example'
    nft_prereveal_json_folder: '0000000000000000000000000000000000000prereveal'
  mainnet:
    btc_usd_price_feed: '0xA39434A63A52E749F02807ae27335515BA4b07F7'
    marketing_address: '0x000000000000000000000000000000000example'
    nft_json_folder: '000000000000000000000000000000000000000example'
    nft_prereveal_json_folder: '0000000000000000000000000000000000000prereveal'
    accessories_address: '0x000000000000000000000000000000000000acce'
    accommodation_address: '0x000000000000000000000000000000000000acco'
